{
  "name": "openfront-client",
  "scripts": {
    "build-dev": "webpack --config webpack.config.js --mode development",
    "build-prod": "webpack --config webpack.config.js --mode production",
    "start:client": "webpack serve --open --node-env development",
    "start:server": "node --loader ts-node/esm --experimental-specifier-resolution=node src/server/Server.ts",
    "start:server-dev": "cross-env GAME_ENV=dev node --loader ts-node/esm --experimental-specifier-resolution=node src/server/Server.ts",
    "dev": "cross-env GAME_ENV=dev concurrently \"pnpm start:client\" \"pnpm start:server-dev\"",
    "dev:backend": "pnpm start:server-dev",
    "dev:client": "pnpm start:client",
    "dev:staging": "cross-env GAME_ENV=dev API_DOMAIN=api.openfront.dev concurrently \"pnpm start:client\" \"pnpm start:server-dev\"",
    "dev:prod": "cross-env GAME_ENV=dev API_DOMAIN=api.openfront.io concurrently \"pnpm start:client\" \"pnpm start:server-dev\"",
    "tunnel": "pnpm build-prod && pnpm start:server",
    "test": "jest",
    "perf": "pnpm tsx tests/perf/*.ts",
    "test:coverage": "jest --coverage",
    "format": "prettier --ignore-unknown --write .",
    "format:map-generator": "cd map-generator && go fmt .",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "husky",
    "gen-maps": "cd map-generator && go run . && pnpm format"
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "prettier --ignore-unknown --write"
    ]
  },
